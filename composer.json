{
    "name": "juzyio/yii2-ckeditor",
    "keywords": [
        "yii",
        "wysiwyg",
        "editor",
        "CKEditor"
    ],
    "description": "Yii2 CKEditor",
    "license": "BSD-3-Clause",
    "type": "yii2-extension",
